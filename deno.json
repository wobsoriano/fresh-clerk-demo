{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check . && deno lint . && deno check",
    "dev": "vite",
    "build": "vite build",
    "start": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "fmt": { "semiColons": true, "singleQuote": true },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*"],
  "imports": {
    "fresh": "jsr:@fresh/core@^2.0.0-beta.3",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^0.9.10",
    "preact": "npm:preact@^10.27.1",
    "@preact/signals": "npm:@preact/signals@^2.3.1",
    "vite": "npm:vite@^7.1.3",
    "tailwindcss": "npm:tailwindcss@^3.4.3",
    "@jsrob/fresh-clerk": "jsr:@jsrob/fresh-clerk@^0.3.3"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ]
  },
  "nodeModulesDir": "auto"
}
